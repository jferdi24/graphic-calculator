{
  "name": "graphic-calculator",
  "version": "1.0.0",
  "description": "En este proyecto puedes hacer un gráfico de barras estático de 5 series en X y graficar un punto en un plano cartesiano",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prod": "NODE_ENV=production npx tailwindcss -i ./src/css/style.css -o ./dist/css/style.css  --postcss --minify",
    "dev": "npx tailwindcss -i ./src/css/style.css -o ./dist/css/style.css  --postcss",
    "build": "export NODE_ENV=production && npx tailwindcss -i ./src/css/style.css -o ./dist/css/style.css  --postcss --minify && webpack --mode production",
    "watch": "npx tailwindcss -i ./src/css/style.css -o ./dist/css/style.css  --postcss && webpack",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jferdi24/graphic-calculator.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jferdi24/graphic-calculator/issues"
  },
  "homepage": "https://github.com/jferdi24/graphic-calculator#readme",
  "devDependencies": {
    "autoprefixer": "^10.3.7",
    "css-loader": "^6.4.0",
    "postcss": "^8.3.9",
    "postcss-cli": "^9.0.1",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.0",
    "style-loader": "^3.3.0",
    "swiper": "^7.0.8",
    "tailwindcss": "^2.2.17",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.3.4"
  }
}